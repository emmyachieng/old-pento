<section class="row">
  <h2>Survey</h2>
</section>

<%= if @demographic do %>
  <%= live_component @socket,
    PentoWeb.DemographicLive.ShowComponent,
    demographic: @demographic %>
<% else %>
    <%= live_component @socket,
      PentoWeb.RatingLive.IndexComponent,
      products: @products, 
      current_user: @current_user %>
<% end %>
